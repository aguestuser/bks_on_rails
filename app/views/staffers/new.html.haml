- provide(:title, 'Create Staffer')
%h1 Create Staffer
.row
  .span6.offset3
    = form_for(@staffer) do |f|
      = render 'shared/error_messages', object: f.object
      = render 'shared/contact_info_fields', form: f, object: f.object
      .center
        = f.submit "Create staffer", class: "btn btn-large btn-primary"      